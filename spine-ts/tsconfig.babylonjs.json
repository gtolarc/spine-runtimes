{
	"compilerOptions": {
		"module": "none",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"outFile": "build/spine-babylonjs.js",
		"sourceMap": true,
		"declaration": true,
		"target": "es6"
	},
	"include": [
		"core/src/**/*",
		"babylonjs/src/**/*"
	],
	"exclude": [
		"webgl",
		"player",
		"canvas",
		"threejs",
		"build"
	]
}
